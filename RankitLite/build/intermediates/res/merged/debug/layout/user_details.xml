<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/flowers"
    >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="400dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:alpha=".9"
        android:background="#ffffff"
        tools:ignore="UnusedAttribute,RtlHardcoded" >

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView02"
            android:layout_below="@+id/textView1"
            android:text=""
            android:textColor="#000000"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="Melissa Black"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView1"
            android:layout_below="@+id/textView1"
            android:text="Gold ranker"
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView05"
            android:layout_alignBottom="@+id/TextView05"
            android:layout_toRightOf="@+id/TextView05"
            android:text=" 01 "
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView06"
            android:layout_alignBottom="@+id/TextView06"
            android:layout_toRightOf="@+id/TextView06"
            android:text="circle"
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/TextView04"
            android:layout_below="@+id/TextView05"
            android:src="@drawable/ic_action_thumbs_up_down"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/TextView08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/linearLayout1"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:text="My circle"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView08"
            android:layout_below="@+id/TextView08"
            android:layout_marginTop="10dp"
            android:adjustViewBounds="true"
            android:maxHeight="30dp"
            android:maxWidth="30dp"
            android:src="@drawable/logo_eco"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:id="@+id/LinearLayout03"
            android:layout_width="250dp"
            android:layout_height="1dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/imageView5"
            android:layout_marginTop="17dp"
            android:alpha="1"
            android:background="#212121"
            android:orientation="vertical" >

        </LinearLayout>

        <ListView
            android:id="@+id/listView"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView13" >

        </ListView>

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/imageView4"
            android:layout_marginRight="2dp"
            android:adjustViewBounds="true"
            android:maxHeight="20dp"
            android:maxWidth="20dp"
            android:src="@drawable/ic_action_delete"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="250dp"
            android:layout_height="1dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView01"
            android:alpha="1"
            android:background="#212121"
            android:orientation="vertical" >

        </LinearLayout>

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView4"
            android:layout_marginTop="2dp"
            android:layout_toRightOf="@+id/TextView09"
            android:text="Life is beautiful, yeah !"
            android:textColor="#000000"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView01"
            android:layout_alignBottom="@+id/TextView01"
            android:layout_alignLeft="@+id/TextView08"
            android:text="Statut : "
            android:textColor="#000000"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView3"
            android:layout_marginTop="18dp"
            android:layout_toLeftOf="@+id/imageView8"
            android:adjustViewBounds="true"
            android:maxHeight="20dp"
            android:maxWidth="20dp"
            android:src="@drawable/ic_content_create"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/TextView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/LinearLayout03"
            android:layout_alignLeft="@+id/TextView01"
            android:text="Les Conso Ecolo"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/ImageView01"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:layout_above="@+id/TextView09"
            android:layout_alignLeft="@+id/imageView3"
            android:src="@drawable/ic_editor_mode_comment"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/TextView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/imageView8"
            android:layout_alignRight="@+id/TextView02"
            android:text="57 "
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageView3"
            android:layout_toLeftOf="@+id/imageView3"
            android:text="135 "
            android:textColor="#424242"
            android:textSize="12sp"
            android:layout_marginTop="5dp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView04"
            android:layout_below="@+id/TextView04"
            android:text="Active in"
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/imageView8"
            android:layout_alignTop="@+id/imageView6"
            android:src="@drawable/ic_action_thumb_down"
            tools:ignore="ContentDescription"
            android:layout_marginRight="5dp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/TextView13"
            android:layout_toLeftOf="@+id/imageView4"
            android:src="@drawable/ic_action_thumb_up"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/TextView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView10"
            android:text="Criteria"
            android:textColor="#000000"
            android:layout_marginLeft="5dp"
            android:textSize="12sp"
            android:layout_marginTop="5dp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/imageView5"
            android:layout_below="@+id/LinearLayout03"
            android:text="My stats"
            android:layout_marginTop="5dp"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/LinearLayout03"
            android:layout_alignLeft="@+id/ImageView01"
            android:text="17 "
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/ImageView02"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:layout_above="@+id/LinearLayout03"
            android:layout_toRightOf="@+id/TextView14"
            android:src="@drawable/ic_editor_mode_comment"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/TextView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/LinearLayout03"
            android:layout_toRightOf="@+id/TextView07"
            android:text="05 "
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/ImageView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/LinearLayout03"
            android:layout_toRightOf="@+id/TextView15"
            android:src="@drawable/ic_action_thumbs_up_down"
            tools:ignore="ContentDescription" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/relativeLayout1"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/relativeLayout1"
        android:layout_marginTop="20dp"
        android:src="@drawable/abc_ic_ab_back_mtrl_am_alpha"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/relativeLayout1"
        android:layout_alignRight="@+id/relativeLayout1"
        android:layout_alignTop="@+id/imageView9"
        android:contentDescription="TODO"
        android:src="@drawable/abc_ic_menu_moreoverflow_mtrl_alpha"
        tools:ignore="HardcodedText,ContentDescription" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView9"
        android:layout_marginTop="14dp"
        android:layout_toRightOf="@+id/imageView9"
        android:layout_toEndOf="@+id/imageView9"
        android:adjustViewBounds="true"
        android:maxHeight="90dp"
        android:maxWidth="90dp"
        android:src="@drawable/girl"
        tools:ignore="ContentDescription" />

</RelativeLayout>