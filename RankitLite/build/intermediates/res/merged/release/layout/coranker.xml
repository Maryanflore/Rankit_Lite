<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/flowers"
     >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="400dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:alpha=".9"
        android:background="#ffffff"
        tools:ignore="UnusedAttribute,RtlHardcoded" >

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView02"
            android:layout_below="@+id/textView1"
            android:text=""
            android:textColor="#000000"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="Melissa Black"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView1"
            android:layout_below="@+id/textView1"
            android:text="Gold ranker"
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView03"
            android:layout_below="@+id/TextView04"
            android:text="Active in"
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView05"
            android:layout_alignBottom="@+id/TextView05"
            android:layout_toRightOf="@+id/TextView05"
            android:text=" 08 "
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView06"
            android:layout_alignBottom="@+id/TextView06"
            android:layout_toRightOf="@+id/TextView06"
            android:text="circles"
            android:textColor="#424242"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView04"
            android:layout_below="@+id/TextView05"
            android:text="+100 "
            android:textColor="#424242"
            android:textSize="12sp"
            android:layout_marginTop="4dp"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/TextView04"
            android:layout_below="@+id/TextView05"
            android:src="@drawable/ic_action_thumbs_up_down"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="2dp"
            android:layout_marginTop="2dp"
            android:src="@android:drawable/presence_online"
            tools:ignore="ContentDescription" />

        <Button
            android:id="@+id/button1"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/imageView3"
            android:layout_marginTop="18dp"
            android:background="#73c2fb"
            android:text="+ circle"
            android:textColor="#ffffff"
            android:layout_marginLeft="40dp"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/Button01"
            android:layout_width="90dp"
            android:layout_height="30dp"
             
            android:layout_alignBottom="@+id/button1"
            android:layout_alignParentRight="true"
            android:background="#73c2fb"
            android:text="Message"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:layout_marginRight="40dp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/button1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:text="Life is beautiful, yeah !"
            android:textColor="#000000"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="250dp"
            android:layout_height="1dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView01"
            android:alpha="1"
            android:layout_marginTop="5dp"
            android:background="#212121"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/LinearLayout01"
                android:layout_width="250dp"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:alpha="1"
                android:background="#212121"
                android:orientation="vertical" >
            </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/TextView08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/linearLayout1"
            android:text="You have"
            android:textColor="#000000"
            android:textSize="12sp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView08"
            android:layout_alignBottom="@+id/TextView08"
            android:layout_toRightOf="@+id/TextView08"
            android:text=" 01 "
            android:textColor="#000000"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView08"
            android:layout_below="@+id/TextView08"
            android:layout_marginTop="10dp"
            android:adjustViewBounds="true"
            android:maxHeight="30dp"
            android:maxWidth="30dp"
            android:src="@drawable/logo_eco"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/TextView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView5"
            android:layout_alignLeft="@+id/button1"
            android:text="Les Conso Ecolo"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <LinearLayout
            android:id="@+id/LinearLayout03"
            android:layout_width="250dp"
            android:layout_height="1dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/imageView5"
            android:layout_marginTop="17dp"
            android:alpha="1"
            android:background="#212121"
            android:orientation="vertical" >
        </LinearLayout>

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/imageView5"
            android:layout_below="@+id/LinearLayout03"
            android:adjustViewBounds="true"
            android:maxHeight="40dp"
            android:maxWidth="40dp"
            android:src="@drawable/happy"
            android:layout_marginTop="10dp"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/TextView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageView2"
            android:layout_toRightOf="@+id/TextView08"
            android:text="HAPPY RANKER"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/imageView5"
            android:layout_toRightOf="@+id/TextView09"
            android:text="shared circle"
            android:textColor="#000000"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/TextView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView2"
            android:layout_marginTop="17dp"
            android:layout_toLeftOf="@+id/TextView12"
            android:text="Criteria"
            android:textColor="#000000"
            android:textSize="12sp"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/imageView4"
            android:layout_alignTop="@+id/imageView6"
            android:src="@drawable/ic_action_thumb_down"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/TextView13"
            android:layout_alignRight="@+id/Button01"
            android:src="@drawable/ic_action_thumb_up"
            tools:ignore="ContentDescription" />

        <ListView
            android:id="@+id/listView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/TextView13" >

        </ListView>

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/relativeLayout1"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/relativeLayout1"
        android:layout_marginTop="20dp"
        android:src="@drawable/abc_ic_ab_back_mtrl_am_alpha"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/relativeLayout1"
        android:layout_alignRight="@+id/relativeLayout1"
        android:layout_alignTop="@+id/imageView9"
        android:contentDescription="TODO"
        android:src="@drawable/abc_ic_menu_moreoverflow_mtrl_alpha"
        tools:ignore="HardcodedText,ContentDescription" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView9"
        android:layout_marginTop="14dp"
        android:layout_toRightOf="@+id/imageView9"
        android:layout_toEndOf="@+id/imageView9"
        android:adjustViewBounds="true"
        android:maxHeight="90dp"
        android:maxWidth="90dp"
        android:src="@drawable/girl"
        tools:ignore="ContentDescription" />

</RelativeLayout>